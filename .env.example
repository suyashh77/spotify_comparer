# Copy this file to `.env` and fill in your values.
# Do NOT commit your real `.env` to GitHub.

SPOTIPY_CLIENT_ID=your_client_id
SPOTIPY_CLIENT_SECRET=your_client_secret
SPOTIPY_REDIRECT_URI=http://127.0.0.1:8888/

# Absolute path to your local music folder
LOCAL_MUSIC_DIR=C:\Users\YourName\Music

# The Spotify playlist to compare against your local folder
SPOTIFY_PLAYLIST_ID=your_playlist_id
